#include <iostream>
using namespace std ;

int cola[6];
int n =6 ;
int front=0;
int rear=0 ;
int x ;

void agregar(int x );
void eliminar();
void mostrar();

int main(){
  agregar(23);
 agregar(2);

    mostrar();
    cout<<" "<<endl;
    eliminar();
    mostrar();
        cout<<" "<<endl;


    return 0;
}
void eliminar() {
    if (front == rear) {
        cout << "Cola vacÃ­a, no se puede eliminar." << endl;
        return;
    }
    cout << "Elemento eliminado: " << cola[front] << endl;
    front = (front + 1) % n;
}
void agregar(int x){
    int next= (rear+1)%n ;
        
     cola[rear]=x ;
        rear = (rear + 1) % n;
        
    }

void mostrar(){
    for (int i = front; i != rear; i = (i + 1) % n) {
        cout<<cola[i]<<" ";
    }
}

